{
  "name": "tongso/geetest",
  "description": "适用于thinkphp6的geetest库",
  "version" : "1.0.0",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Lukin",
      "email": "6306643@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "topthink/think": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "tongso\\geetest\\": "src/"
    },
    "files" : [ "src/helper.php" ]
  },
  "autoload-dev": {
    "psr-4": {
      "tongso\\geetest\\tests\\": "tests/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "tongso\\geetest\\GeetestService"
      ],
      "config": {
         "geetest": "src/config/geetest.php"
      }
    }
  }

}